<% if logged_in? %>
  <div class="row">
    <aside class="col-md-4">
    </aside>
    <div class="col-md-8">
      <h3>Feed</h3>
      <%= render 'shared/feed' %>
    </div>
  </div>
<% else %>
  <div class="center jumbotron">
    <h1>Welcome to the Posts</h1>

    <%= link_to "Sign up now!", signup_path, class: "btn btn-lg btn-danger" %>
  </div>
<% end %>